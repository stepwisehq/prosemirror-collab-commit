{
  "name": "@stepwisehq/prosemirror-collab-commit",
  "version": "1.0.4",
  "description": "Commit-based collab plugin for ProseMirror",
  "homepage": "https://github.com/stepwisehq/prosemirror-collab-commit",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "scripts": {
    "test": "node --inspect --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "tsc"
  },
  "keywords": [
    "prosemirror"
  ],
  "author": "ProTip",
  "license": "MIT",
  "dependencies": {
    "prosemirror-state": "^1.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@swc/core": "^1.3.29",
    "@swc/jest": "^0.2.24",
    "jest": "^29.4.1",
    "jest-watch-typeahead": "^2.2.2",
    "prosemirror-history": "^1.3.0",
    "prosemirror-model": "^1.19.0",
    "prosemirror-test-builder": "^1.1.0",
    "prosemirror-transform": "^1.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
